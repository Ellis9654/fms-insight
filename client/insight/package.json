{
  "name": "insight",
  "version": "0.1.0",
  "license": "BSD-3-Clause",
  "private": true,
  "sideEffects": [
    "./src/index.tsx"
  ],
  "scripts": {
    "start": "parcel src/index.html",
    "build": "tsc --noEmit && parcel build src/index.html",
    "build-demo": "parcel build --out-dir demo --public-url /demo/ src/demo.html",
    "test": "jest --ci --runInBand",
    "test:watch": "jest --watch",
    "nswag": "./node_modules/nswag/bin/nswag.js swagger2tsclient /runtime:NetCore20 /input:../../server/fms-insight-api-1.5.json /output:src/data/api.ts"
  },
  "dependencies": {
    "@material-ui/core": "^3.0.0",
    "@material-ui/icons": "^3.0.0",
    "date-fns": "^1.29.0",
    "downshift": "^2.0.0",
    "history": "^4.7.2",
    "immutable": "^4.0.0-rc.9",
    "jdenticon": "^2.0.0",
    "lodash": "^4.17.10",
    "moment": "^2.20.0",
    "query-string": "^6.0.0",
    "react": "^16.4.0",
    "react-document-title": "^2.0.0",
    "react-dom": "^16.4.0",
    "react-qr-reader": "^2.1.0",
    "react-redux": "^5.0.0",
    "react-timeago": "^4.0.0",
    "react-vis": "^1.8.2",
    "reconnecting-websocket": "^4.0.0-rc5",
    "redux": "^4.0.0",
    "redux-first-router": "^0.0.16-next",
    "reselect": "^3.0.1",
    "typeface-roboto": "^0.0.54"
  },
  "devDependencies": {
    "@types/enzyme": "^3.1.8",
    "@types/enzyme-adapter-react-16": "^1.0.1",
    "@types/faker": "^4.1.2",
    "@types/history": "^4.6.2",
    "@types/jdenticon": "^1.3.1",
    "@types/jest": "^23.0.0",
    "@types/lodash": "^4.14.116",
    "@types/node": "^10.3.0",
    "@types/query-string": "^6.0.0",
    "@types/react": "^16.0.35",
    "@types/react-document-title": "^2.0.0",
    "@types/react-dom": "^16.0.3",
    "@types/react-redux": "^6.0.0",
    "@types/redux-first-router": "^1.10.2",
    "faker": "^4.1.0",
    "jest": "^23.4.2",
    "jest-dom": "^1.12.0",
    "nswag": "^11.14.0",
    "parcel-bundler": "^1.9.7",
    "parcel-plugin-sw-precache": "^1.0.1",
    "prettier": "^1.14.2",
    "react-testing-library": "^5.0.0",
    "source-map-explorer": "^1.5.0",
    "ts-jest": "^23.1.3",
    "tslint": "^5.11.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.0.0"
  },
  "jest": {
    "testURL": "http://localhost",
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest",
      ".*\\.svg$": "<rootDir>/file-mock.js",
      ".*events-json.txt$": "<rootDir>/file-mock.js"
    },
    "preset": "ts-jest",
    "testMatch": null
  },
  "sw-precache": {
    "navigateFallback": "index.html",
    "navigateFallbackWhitelist": [
      "^(?!/(swagger|api|instructions)/).*"
    ]
  }
}
